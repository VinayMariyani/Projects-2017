<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">


	<welcome-file-list>
		<welcome-file>homepage.jsp</welcome-file>
	</welcome-file-list>

	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<description>
		</description>
		<display-name>AddEmployee</display-name>
		<servlet-name>AddEmployee</servlet-name>
		<servlet-class>com.application.servlets.AddEmployee</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ViewEmployee</display-name>
		<servlet-name>ViewEmployee</servlet-name>
		<servlet-class>com.application.servlets.ViewEmployee</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ViewAll</display-name>
		<servlet-name>ViewAll</servlet-name>
		<servlet-class>com.application.servlets.ViewAll</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddEmployee</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewEmployee</servlet-name>
		<url-pattern>/view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewAll</servlet-name>
		<url-pattern>/viewAll</url-pattern>
	</servlet-mapping>
</web-app>
